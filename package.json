{
  "name": "algorithm-architect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . --write",
    "db.create": "docker create --name algorithm-architect-db --env-file .env -p 5432:5432 bitnami/postgresql",
    "db.start": "docker start -i algorithm-architect-db",
    "generate": "npx prisma generate",
    "db.push": "npx prisma db push",
    "db.seed": "npx tsx ./prisma/seed.ts",
    "db.studio": "npx prisma studio",
    "db.migrate": "npx prisma migrate dev",
    "db.reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "^3.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@prisma/client": "^6.5.0",
    "daisyui": "^5.0.9",
    "dotenv": "^16.4.7",
    "http-status-codes": "^2.3.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "use-immer": "^0.11.0",
    "validator": "^13.15.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.17",
    "typescript": "^5"
  }
}
